  <rdf:Description rdf:about="http://escience.rpi.edu/ontology/sesf/s2s/4/0/forInput">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A property that associates s2s:InputValuesInterface instances with the s2s:Input instances they can be used for.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://escience.rpi.edu/ontology/sesf/s2s/4/0/forInput">
    <rdfs:domain rdf:resource="http://escience.rpi.edu/ontology/sesf/s2s/4/0/InputValuesInterface"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://escience.rpi.edu/ontology/sesf/s2s/4/0/forInput">
