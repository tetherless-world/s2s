  <rdf:Description rdf:about="http://escience.rpi.edu/ontology/sesf/s2s/4/0/">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://escience.rpi.edu/ontology/sesf/s2s/4/0/">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This ontology provides a vocabulary used by the S2S framework.  The S2S framework is a sub-project of the NSF-funded Semantic eScience Framework project, focused on automating the construction of search interfaces to Semantic Web Services.  The ontology includes constructs for defining Semantic Web services and relating them to semantically-described user interface components.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://escience.rpi.edu/ontology/sesf/s2s/4/0/">
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">3.0</owl:versionInfo>
  </rdf:Description>
  <rdf:Description rdf:about="http://escience.rpi.edu/ontology/sesf/s2s/4/0/Input">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://escience.rpi.edu/ontology/sesf/s2s/4/0/Input">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A class for Web service inputs to a s2s:Service.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://escience.rpi.edu/ontology/sesf/s2s/4/0/Input">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://escience.rpi.edu/ontology/sesf/s2s/4/0/InputValuesInterface">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://escience.rpi.edu/ontology/sesf/s2s/4/0/InputValuesInterface">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A subclass of s2s:Interface used to retrieve values that can be used to constrain a s2s:Input.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://escience.rpi.edu/ontology/sesf/s2s/4/0/InputValuesInterface">
    <rdfs:subClassOf rdf:resource="http://escience.rpi.edu/ontology/sesf/s2s/4/0/Interface"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://escience.rpi.edu/ontology/sesf/s2s/4/0/InputWidget">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://escience.rpi.edu/ontology/sesf/s2s/4/0/InputWidget">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A user interface component to facilitate s2s:Input constraints by the user, potentially using the results of a call to a s2s:InputValuesInterface.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://escience.rpi.edu/ontology/sesf/s2s/4/0/InputWidget">
    <rdfs:subClassOf rdf:resource="http://escience.rpi.edu/ontology/sesf/s2s/4/0/Widget"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="genid1">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Restriction"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="genid1">
    <owl:onClass rdf:resource="http://escience.rpi.edu/ontology/sesf/s2s/4/0/Output"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="genid1">
    <owl:onProperty rdf:resource="http://escience.rpi.edu/ontology/sesf/s2s/4/0/hasOutput"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="genid1">
    <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
  </rdf:Description>
  <rdf:Description rdf:about="http://escience.rpi.edu/ontology/sesf/s2s/4/0/Interface">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://escience.rpi.edu/ontology/sesf/s2s/4/0/Interface">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A class for interfaces/operations implemented by a s2s:SearchService.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://escience.rpi.edu/ontology/sesf/s2s/4/0/Interface">
    <rdfs:subClassOf rdf:nodeID="genid1"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://escience.rpi.edu/ontology/sesf/s2s/4/0/Output">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://escience.rpi.edu/ontology/sesf/s2s/4/0/Output">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A class for identifying Web service response formats.  This class is used to align s2s:Interface instances with the s2s:Widget instances that support them.  In the future, more semantics should be added to s2s:Output to allow for more flexible alignment of s2s:Widgets with s2s:Interfaces.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://escience.rpi.edu/ontology/sesf/s2s/4/0/Output">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://escience.rpi.edu/ontology/sesf/s2s/4/0/Paradigm">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://escience.rpi.edu/ontology/sesf/s2s/4/0/Paradigm">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A class for describing search paradigms such as faceted search or hierarchical search.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://escience.rpi.edu/ontology/sesf/s2s/4/0/ResultsWidget">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://escience.rpi.edu/ontology/sesf/s2s/4/0/ResultsWidget">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A user interface component used to display the output of a call to a s2s:SearchResultsInterface.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://escience.rpi.edu/ontology/sesf/s2s/4/0/ResultsWidget">
    <rdfs:subClassOf rdf:resource="http://escience.rpi.edu/ontology/sesf/s2s/4/0/Widget"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://escience.rpi.edu/ontology/sesf/s2s/4/0/SearchResultsInterface">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://escience.rpi.edu/ontology/sesf/s2s/4/0/SearchResultsInterface">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A subclas of s2s:Interface used to provide results for a s2s:SearchService.  In other words, the results of a call to this interface provide no information on how to call other interfaces for the same service (unlike s2s:InputValuesInterface).</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://escience.rpi.edu/ontology/sesf/s2s/4/0/SearchResultsInterface">
    <rdfs:subClassOf rdf:resource="http://escience.rpi.edu/ontology/sesf/s2s/4/0/Interface"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://escience.rpi.edu/ontology/sesf/s2s/4/0/SearchService">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://escience.rpi.edu/ontology/sesf/s2s/4/0/SearchService">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A class for describing Web services for search.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://escience.rpi.edu/ontology/sesf/s2s/4/0/SearchService">
    <rdfs:subClassOf rdf:resource="http://escience.rpi.edu/ontology/sesf/s2s/4/0/Service"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://escience.rpi.edu/ontology/sesf/s2s/4/0/Service">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://escience.rpi.edu/ontology/sesf/s2s/4/0/Service">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A class for describing Web services.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://escience.rpi.edu/ontology/sesf/s2s/4/0/Widget">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://escience.rpi.edu/ontology/sesf/s2s/4/0/Widget">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A semantic description of a user interface component in S2S, including s2s:Outputs (Web service responses) supported.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://escience.rpi.edu/ontology/sesf/s2s/4/0/forInput">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
