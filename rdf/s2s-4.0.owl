  <rdf:Description rdf:about="http://escience.rpi.edu/ontology/sesf/s2s/4/0/hasOutput">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://escience.rpi.edu/ontology/sesf/s2s/4/0/hasOutput">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A property that associates a s2s:Interface with the s2s:Output it returns.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://escience.rpi.edu/ontology/sesf/s2s/4/0/hasOutput">
    <rdfs:domain rdf:resource="http://escience.rpi.edu/ontology/sesf/s2s/4/0/Interface"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://escience.rpi.edu/ontology/sesf/s2s/4/0/hasOutput">
