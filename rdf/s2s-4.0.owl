  </rdf:Description>
  <rdf:Description rdf:about="http://escience.rpi.edu/ontology/sesf/s2s/4/0/supportsInput">
    <rdfs:domain rdf:resource="http://escience.rpi.edu/ontology/sesf/s2s/4/0/InputWidget"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://escience.rpi.edu/ontology/sesf/s2s/4/0/supportsInput">
